@{
    ViewBag.Title = "User Portal";
    Layout = "~/Views/Shared/_UserPortalLayout.cshtml";
}

<!--Header/Breadcrumb-->
<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">
            Grant Writing
            <small>Edit Grants</small>
        </h1>
        <ol class="breadcrumb">
            <li>
                <i class="fa fa-fw fa-dashboard"></i> @Html.ActionLink("Dashboard", "Index", "UserPortal")
            </li>
            <li class="active">
                <i class="fa fa-fw fa-bar-chart-o"></i> @Html.ActionLink("Grant Writing", "Index", "Grant")
            </li> 
            <li class="active">
                <i class="fa fa-fw fa-bar-chart-o"></i> Edit Grants
            </li> 
         </ol>
    </div>
</div>

<a class="btn btn-link" href=@Url.Action("Create", "Grant") role="button">Create Grant</a>
<a class="btn btn-link" href=@Url.Action("EditGrants", "Grant") role="button">Edit Grants</a>
<a class="btn btn-link" href=@Url.Action("ViewGrants", "Grant") role="button">View Grants</a>

<!--Body-->
    <p>Text</p>
