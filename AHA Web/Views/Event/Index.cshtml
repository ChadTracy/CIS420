            @Html.DisplayFor(modelItem => item.Event_Type)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Event_Start_Date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Event_End_Date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Event_Time)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Event_ID }) |
