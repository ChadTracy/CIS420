<style>
    .scheduler_container {
        height: 500px;
        width: 1100px;
    }
</style>
@{
    ViewBag.Title = "Events";
    Layout = "~/Views/Shared/_UserPortalLayout.cshtml";
}
<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">
            Events
            <small>Event Calendar</small>
        </h1>
        <ol class="breadcrumb">
            <li>
                <i class="fa fa-fw fa-dashboard"></i> <a href="@Url.Action("Index", "UserPortal")"> Dashboard</a>
            </li>

            <li class="active">
                <i class="fa fa-fw fa-table"></i> Event Calendar
            </li>

        </ol>
    </div>
</div>
<div class="scheduler_container">
    @Html.Raw(Model.Render())
</div>